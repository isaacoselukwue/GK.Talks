{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RegistrationRules": {
    "OutdatedTechnologies": [ "Cobol", "Punch Cards", "Commodore", "VBScript" ],
    "AllowedEmployers": [ "Pluralsight", "Microsoft", "Google" ],
    "BlockedEmailDomains": [ "aol.com", "prodigy.com", "compuserve.com" ],
    "CertificationsRequired": 3,
    "MinimumInternetExplorerMajorVersion": 9
  },
  "ConnectionStrings": {
    "Default": "DataSource=:memory:"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "fileSizeLimitBytes": 10000000,
          "path": "logs/log.txt",
          "rollingInterval": "Hour",
          "rollOnFileSizeLimit": true,
          "retainedFileCountLimit": 1000,
          "retainedFileTimeLimit": "5.00:00:00"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
